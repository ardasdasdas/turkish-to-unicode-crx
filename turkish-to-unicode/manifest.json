{
  "manifest_version": 3,
  "name": "convertToTurkishUnicode",
  "version": "2.0.0",
  "description": "Türkçe karakterleri Unicode ile değiştirir.",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  }
}
